{
  "name": "image-scanner",
  "version": "1.0.0",
  "description": "Image Scanner Test Bed",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron-forge start",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make"
  },
  "author": "distriqt Pty Ltd",
  "license": "ISC",
  "dependencies": {
    "@nodert-win10-rs4/windows.devices.enumeration": "^0.4.4",
    "@nodert-win10-rs4/windows.devices.scanners": "^0.4.4",
    "@nodert-win10-rs4/windows.storage": "^0.4.4",
    "@nodert-win10-rs4/windows.storage.fileproperties": "^0.4.4",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "path": "^0.12.7",
    "promisify": "^0.0.3",
    "ws": "^8.7.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^21.0.1",
    "eslint": "^7.32.0",
    "typescript": "^4.8.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "officeworks_broker"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
